{
  "version": 2,
  "buildCommand": "cd frontend && npm run build && cd ../backend && npm install",
  "outputDirectory": "frontend/dist",
  "functions": {
    "backend/index.js": {
      "runtime": "@vercel/node@2"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
